<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.RadioMenuItem?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="connectionMenuBarPane" prefHeight="40.0" prefWidth="101.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <MenuBar prefHeight="25.0" prefWidth="108.0">
         <menus>
            <Menu fx:id="menuLang" mnemonicParsing="false" text="%menubar.lang">
               <items>
                  <RadioMenuItem fx:id="ru" mnemonicParsing="false" text="%menubar.lang.russian" />
                  <RadioMenuItem fx:id="en" mnemonicParsing="false" text="%menubar.lang.english" />
                  <RadioMenuItem fx:id="nl" mnemonicParsing="false" text="%menubar.lang.dutch" />
                  <RadioMenuItem fx:id="se" mnemonicParsing="false" text="%menubar.lang.swedish" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
   </children>
</StackPane>
