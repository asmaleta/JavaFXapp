<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox maxHeight="650.0" maxWidth="1280.0" minHeight="650.0" minWidth="1280.0" prefHeight="650.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <FlowPane alignment="CENTER" columnHalignment="CENTER" hgap="20.0" prefHeight="200.0" prefWidth="1280.0" vgap="20.0">
         <children>
            <JFXButton fx:id="filter_less_than_distance" alignment="CENTER" cache="true" contentDisplay="TEXT_ONLY" onAction="#filterLessThanDistanceButton" prefHeight="50.0" prefWidth="200.0" style="-fx-border-color: black;" text="Вывод маршрутов с дистанцией меньше заданной" textAlignment="CENTER" wrapText="true">
               <FlowPane.margin>
                  <Insets />
               </FlowPane.margin>
               <font>
                  <Font name="Arial" size="10.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="info" alignment="CENTER" cache="true" contentDisplay="TEXT_ONLY" onAction="#infoButton" prefHeight="50.0" prefWidth="200.0" style="-fx-border-color: black;" text="Инфомация о коллекции" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font name="Arial" size="10.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="sum_of_distance" alignment="CENTER" cache="true" contentDisplay="TEXT_ONLY" onAction="#sumOfDistanceButton" prefHeight="50.0" prefWidth="200.0" style="-fx-border-color: black;" text="Сумма длин всех маршрутов" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font name="Arial" size="10.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="history" alignment="CENTER" cache="true" contentDisplay="TEXT_ONLY" onAction="#historyButton" prefHeight="50.0" prefWidth="200.0" style="-fx-border-color: black;" text="История" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font name="Arial" size="10.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="execute_script" alignment="CENTER" cache="true" contentDisplay="TEXT_ONLY" onAction="#scriptButton" prefHeight="50.0" prefWidth="200.0" style="-fx-border-color: black;" text="Выполнить скрипт" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font name="Arial" size="10.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="remove_greater" alignment="CENTER" onAction="#removeGreaterButton" prefHeight="50.0" prefWidth="200.0" style="-fx-border-color: black;" text="Удалить маршрут больше заданного">
               <font>
                  <Font name="Arial" size="10.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="remove_lower" alignment="CENTER" onAction="#removeLowerButton" prefHeight="50.0" prefWidth="200.0" style="-fx-border-color: black;" text="Удалить маршрут меньше заданного">
               <font>
                  <Font name="Arial" size="10.0" />
               </font>
            </JFXButton>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </FlowPane>
      <TextField id="arg" fx:id="arg" prefHeight="25.0" prefWidth="610.0" promptText="%commands.textField.arg" style="-fx-background-color: white; -fx-border-color: black;" stylesheets="@../../styles.css">
         <padding>
            <Insets bottom="10.0" left="10.0" />
         </padding>
         <VBox.margin>
            <Insets bottom="20.0" left="50.0" right="50.0" top="30.0" />
         </VBox.margin>
      </TextField>
      <TextArea fx:id="output" editable="false" prefHeight="463.0" prefWidth="1210.0" promptText="результат запроса" style="-fx-border-color: black;">
         <VBox.margin>
            <Insets bottom="50.0" left="50.0" right="50.0" />
         </VBox.margin>
      </TextArea>
   </children>
</VBox>
